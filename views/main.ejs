<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Main Page</title>
</head>
<body>
    <% feeds.forEach((f) => { %>
        <div>
            <ul>
                <% f.items.forEach((i) => { %>
                    <li><b><%= i.title %></b>
                        <ol style="list-style: none;">
                            <li class="content"><%- i.content %></li>
                        </ol>
                    </li>
                <% }) %>
            </ul>
        </div>
    <% }); %>
    <script>
        document.querySelector('.yes').innerHTML = i.content;
    </script>
</body>
</html>